{
    "name": "vite-plugin-vars-modifier",
    "version": "1.0.0",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/fanhaoyuan/vite-plugin-vars-modifier.git",
    "author": "Fansy <418536538@qq.com>",
    "license": "MIT",
    "description": "A vite plugin for transform variables to config",
    "keywords": [
        "vite",
        "vite-plugin",
        "plugin",
        "less",
        "sass",
        "scss",
        "vars",
        "variables"
    ],
    "scripts": {
        "dev": "vite build --watch",
        "build": "vite build && tsc",
        "test": "jest test"
    },
    "gitHooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "cmv"
    },
    "lint-staged": {
        "*.{md,json}": [
            "prettier --write"
        ],
        "*.{js,ts}": [
            "prettier --write",
            "eslint --fix"
        ]
    },
    "dependencies": {
        "fast-glob": "^3.2.7",
        "less-json-vars": "^1.1.1",
        "lodash-es": "^4.17.21",
        "vite": "^2.5.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@fansy/commit-msg-validator": "^1.0.0",
        "@fansy/eslint-config": "^1.0.0",
        "@fansy/prettier-config": "^1.0.0",
        "@jest/types": "^27.0.6",
        "@types/jest": "^27.0.1",
        "@types/less": "^3.0.3",
        "@types/lodash-es": "^4.17.4",
        "@types/node": "^16.6.0",
        "eslint": "^7.32.0",
        "jest": "^27.0.6",
        "lint-staged": "^11.1.2",
        "prettier": "^2.3.2",
        "ts-node": "^10.2.1",
        "typescript": "^4.3.5"
    }
}
